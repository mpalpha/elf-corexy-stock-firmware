     34          unsigned LCD_Color2Index_M323(LCD_COLOR Color) {
     35            unsigned r, g, b;
     36            r = Color & 255;
     37            g = (Color >> 8 ) & 255;
     38            b = Color >> 16;
     39            r = (r * 7 + 127) / 255;
     40            g = (g + 42) / 85;
     41            b = (b * 7 + 127) / 255;
     42            return b + (g << 3) + (r << 5);
