     42          *       _ShowInvalid
     43          *
     44          * Function:
     45          *   Debug code: shows invalid areas
     46          */
     47          #if (WM_SUPPORT_DIAG)
     48          static void _ShowInvalid(WM_HWIN hWin) {
     49            GUI_CONTEXT Context = GUI_Context;
     50            GUI_RECT rClient;
     51            WM_Obj * pWin;
     52            pWin = WM_H2P(hWin);
     53            rClient = pWin->InvalidRect;
     54            GUI_MoveRect(&rClient, -pWin->Rect.x0, -pWin->Rect.y0);
     55            WM_SelectWindow(hWin);
     56            GUI_SetColor(GUI_GREEN);
     57            GUI_SetBkColor(GUI_GREEN);
     58            GUI_FillRect(rClient.x0, rClient.y0, rClient.x1, rClient.y1);
     59            Sleep(20);
     60            GUI_Context = Context;
     61          }
     62          #endif
